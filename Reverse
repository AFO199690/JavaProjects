package com.thread;

public class Reverse extends Thread {
	public void run()
	{
		try
		{
			System.out.println("\n============");
			System.out.println("\nReverse is:");
			System.out.println("\n============");
			for(int i=10;i>=1;i--)
			{
				System.out.println(i+" ");
			}
			System.out.println("\n==========");
			}
		catch(Exception ex)
		{
			ex.printStackTrace();
			
		}
	}

}
